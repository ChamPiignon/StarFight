<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LaunchPage">
    <children>
        <GridPane layoutX="6.0" layoutY="6.0" prefHeight="236.0" prefWidth="492.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="70.0" text="Starfight" textAlignment="CENTER" GridPane.columnSpan="3">
               <font>
                  <Font size="21.0" />
               </font></Label>
                <TableView prefHeight="79.0" prefWidth="351.0" GridPane.columnSpan="3" GridPane.rowIndex="1">
                    <columns>
                        <TableColumn prefWidth="75.0" text="Name Player" />
                        <TableColumn prefWidth="75.0" text="Nombre de match" />
                        <TableColumn prefWidth="75.0" text="Taux de victoire" />
                        <TableColumn prefWidth="75.0" text="meilleure temps" />
                    </columns>
                </TableView>
                <Button mnemonicParsing="false" text="Play" GridPane.rowIndex="2" onAction="#onClickPlay"/>
                <Button mnemonicParsing="false" text="Exit" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <ComboBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
